{
  "presets": [
    "cozy-app"
  ],
  "plugins": [
    ["css-modules-transform", {
      extensions: [".styl"],
      preprocessCss: "./preprocess",
      extractCss: "./transpiled/stylesheet.css",
      generateScopedName: "[name]__[local]___[hash:base64:5]",
    }],
    ["transform-runtime", { "polyfill": false }]
  ],
  "ignore": ["examples/**/*"]
}
